{
    "name": "dansan/jobboy-processes-redis",
    "description": "Redis ProcessRepository for dansan/jobboy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Sanchi",
            "email": "danilo.sanchi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-redis": "*",
        "dansan/jobboy": "^0.2.0"
    },
    "require-dev": {
        "dansan/fixture-handler": "*",
        "nesbot/carbon": "^2.0",
        "mockery/mockery": "dev-master",
        "symfony/dotenv": "^4.2",
        "symfony/phpunit-bridge": "*",
        "symfony/framework-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 60,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JobBoy\\Process\\": "src/JobBoy/Process/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },

    "minimum-stability": "stable"
}
